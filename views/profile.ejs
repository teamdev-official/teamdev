<div class="content">
   <div class="col-md-8">
        <div class="card card-round">
            <div class="card-header">
                <h5 class="title">Profile</h5>
            </div>
            <div class="card-body">
                <div class="row">
                    <div class="col-md-4 pr-md-1">
                        <div class="form-group">
                            <label>Username</label>
                            <p><%= username %></p>
                        </div>
                    </div>
                    <div class="col-md-4 px-md-1">
                        <div class="form-group">
                            <label>Email</label>
                            <p><%= email %></p>
                        </div>
                    </div>
                    <div class="col-md-4 px-md-1">
                        <div class="form-group">
                            <label>WhatsApp Number</label>
                            <p><%= nomorWa %></p>
                        </div>
                    </div>
                    <div class="col-md-4 pl-md-1">
                        <div class="form-group">
                            <label>Api Key</label>
                            <p><%= apikey %></p>
                        </div>
                    </div>
                 </div>
                <div class="row">
                    <div class="col-md-4 pr-md-1">
                        <div class="form-group">
                            <label>Limit</label>
                            <p><%= limit %></p>
                        </div>
                    </div>
                    <div class="col-md-4 px-md-1">
                        <div class="form-group">
                            <label>Request</label>
                            <p><%= totalreq %></p>
                        </div>
                    </div>
                 </div>
                    
           </div>
       </div>
      <div class="card card-round">
            <div class="card-header ">
                <div class="row">
                    <div class="col-sm-6 text-left">
                        <h5 class="title">Edit Profile</h5>
                    </div>
                    <div class="col-sm-6">
                        <div class="btn-group btn-group-toggle float-right" data-toggle="buttons">
                            <label class="btn btn-sm btn-primary btn-simple active" id="0">
                                <input type="radio" name="options" checked>
                                <span class="d-none d-sm-block d-md-block d-lg-block d-xl-block">Profile</span>
                                <span class="d-block d-sm-none">
                                    <i class="tim-icons icon-single-02"></i>
                                </span>
                            </label>
                            <label class="btn btn-sm btn-primary btn-simple" id="2">
                                <input type="radio" class="d-none" name="options">
                                <span class="d-none d-sm-block d-md-block d-lg-block d-xl-block">Api Key</span>
                                <span class="d-block d-sm-none">
                                    <i class="fi-rr-flame"></i>
                                </span>
                            </label>
                        </div>
                    </div>
                </div>
            </div>
            <div class="card-body" id="usernameemail">
                <form method="POST" action="/profile" class="changeprofile">
                    <div class="row">
                        <div class="col-md-12 pr-md-1">
                            <div class="form-group">
                                <label>Username</label>
                                <input type="text" class="form-control" name="username" placeholder="Username" value='<%= username %>' autocomplete="off" required>
                            </div>
                        </div>
                        <input type="text" name="changeprofile" value="usernameemail" style="display: none;">
                        <button type="submit" class="btn btn-fill btn-primary ml-3 inibutton">Save</button>
                    </div>
                </form>
            </div>
           
            <div class="card-body" id="apikey" style="display: none;">
                <form method="POST" action="/premium/custom" class="changeprofile">
                    <div class="row">
                        <div class="col-md-12 pr-md-1">
                            <div class="form-group">
                               <label>Custom Apikey</label>
                               <input type="text" class="form-control" name="customKey" placeholder="Enter New Apikey" autocomplete="off" required>
                            </div>
                        </div>
                        <input type="text" name="changeprofile" value="apikey" style="display: none;">
                        <button type="submit" class="btn btn-fill btn-primary ml-3 inibutton">Custom</button>
                    </div>
                </form>
            </div>
        </div>
       </div>
       
